<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >

	<include
		android:id="@+id/include_profile_my"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		layout="@layout/include_profile_my" />

	<include
		android:id="@+id/include_profile_people"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		layout="@layout/include_profile_people" />

	<include
		android:id="@+id/include_profile_time"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		layout="@layout/include_profile_time" />

	<View
		android:layout_width="wrap_content"
		android:layout_height="2dp"
		android:background="@color/solid_orange" />

	<include
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		layout="@layout/include_profile_menu" />

</LinearLayout>